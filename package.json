  "standard-version": {
    "issueUrlFormat": "{{host}}/helikarlab/cellcollective/issues/{{id}}",
    "commitUrlFormat": "{{host}}/helikarlab/cellcollective/commit/{{hash}}",
    "types": [
      {
        "type": "feat",
        "section": "Features"
      },
      {
        "type": "fix",
        "section": "Bug Fixes"
      },
      {
        "type": "docs",
        "section": "Docs"
      },
      {
        "type": "perf",
        "section": "Perfomance Changes"
      },
      {
        "type": "test",
        "hidden": true
      },
      {
        "type": "chore",
        "hidden": true
      },
      {
        "type": "refactor",
        "hidden": true
      },
      {
        "type": "style",
        "hidden": true
      }
    ],
    "skip": {
      "tag": true
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
