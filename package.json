{
  "name": "cc",
  "scripts": {
    "build:docs": "$(yarn bin)/jsdoc -r cc -d ./docs/development",
    "pre-build": "yarn run config:generate",
    "build:development": "yarn run pre-build && echo \"running vite at cc/client...\"",
    "build:production": "yarn run pre-build && echo \"running vite at cc/client...\"",
    "build:test": "yarn run build:production",
    "build": "yarn run build:development",
    "start:development": "$(yarn bin)/nodemon --exec babel-node --stack-size=16432 ./cc; # $(yarn bin)/pm2-dev --interpreter $(yarn bin)/babel-node --ignore \"cc/client/app/**/*,cc/private/**/*,cc/migrations/**/*\" ./cc",
    "start:docker": "$(yarn bin)/pm2-runtime --node-args=\"--stack-size=16432\" --interpreter $(yarn bin)/babel-node ./cc",
    "start": "yarn run start:development",
    "install:translator": "pip3 install ./scripts/translate/translator",
    "build:translations": "yarn install:translator && translator -l ./cc/data/languages.json -d ./cc/client/app/locales/data.json -t ./cc/client/app/locales",
    "build:merge-messages": "$(yarn bin)/babel-node ./scripts/translate/mergeMessages.js",
    "build:langs": "yarn build && yarn build:merge-messages && yarn build:translations",
    "test:cypress": "$(yarn bin)/cypress run --project ./tests --headless",
    "config:generate": "$(yarn bin)/babel-node ./scripts/genClientConfig.js",
    "test": "yarn run test:cypress",
    "db": "$(yarn bin)/sequelize --models-path ./cc/models --config cc/config/sequelize/config.json --migrations-path ./cc/migrations",
    "db:migrate": "yarn run db db:migrate",
    "db:migrate:undo": "yarn run db db:migrate:undo",
    "db:seed": "yarn run db db:seed",
    "db:sync": "npx babel-node ./scripts/db/sync.js",
    "lint": "eslint cc/client --ext .js,.jsx",
    "prettier": "npx prettier --write",
    "commit": "npx git-cz",
    "changelog:generate": "npx conventional-changelog-cli -i CHANGELOG.md -o CHANGELOG.md -r 0",
    "release": "npx standard-version",
    "test:spec": "$(yarn bin)/jest"
  },
